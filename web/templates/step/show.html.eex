<h2>Show step</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @step.name %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @step.url %>
  </li>

  <li>
    <strong>Beat:</strong>
    <%= @step.beat %>
